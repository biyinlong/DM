{
	"name": "nx91手机号码输入",
	"init_step": "userInputStep",
	"steps": [{
		"name": "userInputStep",
		"property": "keypadStep",
		"keypadInputParam": "mobileNumber",
		"noParamReply": {
			"interrupt": "true",
			"property": "broadcast",
			"content": "nx00901_01"
		},
		"noMatch": {
			"noMatchorinputlist": [{
				"interrupt": "true",
				"property": "broadcast",
				"content": "nx00901nomatch1"
			},
			{
				"interrupt": "true",
				"property": "broadcast",
				"content": "nx00901nomatch1"
			},
			{
				"interrupt": "false",
				"property": "broadcast",
				"content": "nx00901error3"
			}],
			"actionlist": [{
				"property": "broadcastReply",
				"actionParams": ["true",
				"nx_more"]
			},
			{
				"property": "backToMainMenu",
				"actionParams": []
			}]
		},
		"noInput": {
			"noMatchorinputlist": [{
				"interrupt": "true",
				"property": "broadcast",
				"content": "nx00901timeout2"
			},
			{
				"interrupt": "true",
				"property": "broadcast",
				"content": "nx00901timeout2"
			},
			{
				"interrupt": "false",
				"property": "broadcast",
				"content": "nx00901error3"
			}],
			"actionlist": [{
				"property": "broadcastReply",
				"actionParams": ["true",
				"nx_more"]
			},
			{
				"property": "backToMainMenu",
				"actionParams": []
			}]
		},
		"transitions": [{
			"condition": "mobileNumber != '*' ",
			"actions": [{
				"property": "executeProcessAction",
				"actionParams": ["nx92手机号码确认"]
			}]
		},
		{
			"condition": "mobileNumber == '*' ",
			"actions": [{
				"property": "clearParamValue",
				"actionParams": ["mobileNumber"]
			},
			{
				"property": "broadcastReply",
				"actionParams": ["true",
				"nx_more"]
			},
			{
				"property": "backToMainMenu",
				"actionParams": []
			}]
		}]
	}]
}