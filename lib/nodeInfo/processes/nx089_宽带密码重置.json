{
	"name": "nx089_宽带密码重置",
	"onEnterAction": [{
		"actionParams": ["false",
		"nx089_lb"],
		"property": "broadcastReply"
	}],
	"init_step": "isBroadbandServices",
	"steps": [{
		"property": "APIStep",
		"name": "isBroadbandServices",
		"transitions": [{
			"condition": "isBroadbandServices.是否办理宽带业务 == false ",
			"actions": [{
				"property": "broadcastReply",
				"actionParams": ["false",
				"nx089_01"]
			},
			{
				"property": "broadcastReply",
				"actionParams": ["true",
				"nx_more"]
			},
			{
				"property": "backToMainMenu",
				"actionParams": []
			}]
		},
		{
			"condition": "isBroadbandServices.是否办理宽带业务 == true ",
			"actions": [{
				"property": "executeStepAction",
				"actionParams": ["isStopService"]
			}]
		}],
		"helpReply": null,
		"reListenReply": null,
		"functionEntity": {
			"input": {
				"tel": "$tel$"
			},
			"functionName": "sfblkd",
			"output": ["是否办理宽带业务",
			"宽带账号"]
		}
	},
	{
		"property": "APIStep",
		"name": "isStopService",
		"transitions": [{
			"condition": "isStopService.是否停机 == false ",
			"actions": [{
				"property": "executeStepAction",
				"actionParams": ["isRealName"]
			}]
		},
		{
			"condition": "isStopService.是否停机 == true && isStopService.停机原因 == '欠费停机'",
			"actions": [{
				"property": "broadcastReply",
				"actionParams": ["false",
				"nx901_02"]
			},
			{
				"property": "broadcastReply",
				"actionParams": ["true",
				"nx_more"]
			},
			{
				"property": "backToMainMenu",
				"actionParams": []
			}]
		},
		{
			"condition": "isStopService.是否停机 == true && isStopService.停机原因 == '主动报停'",
			"actions": [{
				"property": "broadcastReply",
				"actionParams": ["false",
				"nx901_01"]
			},
			{
				"property": "broadcastReply",
				"actionParams": ["true",
				"nx_more"]
			},
			{
				"property": "backToMainMenu",
				"actionParams": []
			}]
		},
		{
			"condition": "isStopService.是否停机 == true && isStopService.停机原因 == '其他原因'",
			"actions": [{
				"property": "synthesisReply",
				"actionParams": ["false",
				"您好！您的手机由于$isStopService.其他原因之具体$原因！无法办理此业务！"]
			},
			{
				"property": "broadcastReply",
				"actionParams": ["true",
				"nx_more"]
			},
			{
				"property": "backToMainMenu",
				"actionParams": []
			}]
		}],
		"helpReply": null,
		"reListenReply": null,
		"functionEntity": {
			"input": {
				"tel": "$tel$"
			},
			"functionName": "tjpd",
			"output": ["是否停机",
			"停机原因"]
		}
	},
	{
		"property": "APIStep",
		"name": "isRealName",
		"transitions": [{
			"condition": "isRealName.是否实名 == false ",
			"actions": [{
				"property": "broadcastReply",
				"actionParams": ["false",
				"nx089_02"]
			},
			{
				"property": "broadcastReply",
				"actionParams": ["true",
				"nx_more"]
			},
			{
				"property": "backToMainMenu",
				"actionParams": []
			}]
		},
		{
			"condition": "isRealName.是否实名 == true ",
			"actions": [{
				"property": "executeProcessAction",
				"actionParams": ["nx902密码验证流程"]
			},
			{
				"property": "executeProcessAction",
				"actionParams": ["nx08901宽带密码重置确认"]
			},{
				"property": "executeStepAction",
				"actionParams": ["isResetSuccess"]
			}]
		}
		],
		"helpReply": null,
		"reListenReply": null,
		"functionEntity": {
			"input": {
				"tel": "$tel$"
			},
			"functionName": "smrz",
			"output": ["是否实名"]
		}
	},
	{
		"property": "APIStep",
		"name": "isResetSuccess",
		"transitions": [{
			"condition": "isResetSuccess.是否重置成功 == false ",
			"actions": [{
				"property": "broadcastReply",
				"actionParams": ["false",
				"nx089_04"]
			},
			{
				"property": "backToMainMenu",
				"actionParams": []
			}]
		},
		{
			"condition": "isResetSuccess.是否重置成功 == true ",
			"actions": [{
				"property": "broadcastReply",
				"actionParams": ["false",
				"nx089_03"]
			},
			{
				"property": "broadcastReply",
				"actionParams": ["true",
				"nx_busi_success"]
			},
			{
				"property": "backToMainMenu",
				"actionParams": []
			}]
		}],
		"helpReply": null,
		"reListenReply": null,
		"functionEntity": {
			"input": {
				"tel":"$tel$",
				"broadband": "$isBroadbandServices.宽带账号$"
			},
			"functionName": "kdmmcz",
			"output": ["是否重置成功"]
		}
	}]
}