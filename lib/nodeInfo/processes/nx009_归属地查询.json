{
	"name": "nx009_归属地查询",
	"onEnterAction": [{
		"actionParams": ["false",
		"nx009_lb"],
		"property": "broadcastReply"
	},
	{
		"property": "executeProcessAction",
		"actionParams": ["nx91手机号码输入"]
	}],
	"init_step": "isQuerySuccessStep",
	"steps": [{
		"property": "APIStep",
		"name": "isQuerySuccessStep",
		"functionEntity": {
			"input": {
				"tel": "$mobileNumber$"
			},
			"functionName": "gsdcx",
			"output": ["是否存在归属地记录",
			"归属地内容",
			"归属地短信内容"]
		},
		"transitions": [{
			"condition": "isQuerySuccessStep.是否存在归属地记录==true",
			"actions": [{
				"actionParams": ["false",
				"$isQuerySuccessStep.归属地内容$"],
				"property": "synthesisReply"
			},
			{
				"property": "executeStepAction",
				"actionParams": ["sendMess"]
			},
			{
				"property": "broadcastReply",
				"actionParams": ["true",
				"nx_busi_success"]
			},
			{
				"property": "backToMainMenu",
				"actionParams": []
			}]
		},
		{
			"condition": "isQuerySuccessStep.是否存在归属地记录==false",
			"actions": [{
				"actionParams": ["false",
				"nx009_01"],
				"property": "broadcastReply"
			},
			{
				"property": "broadcastReply",
				"actionParams": ["true",
				"nx_more"]
			},
			{
				"property": "backToMainMenu",
				"actionParams": []
			}]
		}]
	},
	{
		"name": "sendMess",
		"property": "APIStep",
		"functionEntity": {
			"input": {
				"tel": "$tel$",
				"content": "$isQuerySuccessStep.归属地短信内容$"
			},
			"functionName": "dxxf",
			"output": []
		}
	}]
}