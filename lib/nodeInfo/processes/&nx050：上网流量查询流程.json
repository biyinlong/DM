{
	"name": "&nx050：上网流量查询流程",
	"onEnterAction": [{
		"actionParams": ["false",
		"nx050_lb"],
		"property": "broadcastReply"
	}],
	"init_step": "isHaveBPInfo",
	"steps": [{
		"property": "APIStep",
		"name": "isHaveBPInfo",
		"transitions": [{
			"condition": "isHaveBPInfo.是否存在记录 == false ",
			"actions": [{
				"property": "broadcastReply",
				"actionParams": ["false",
				"nx050_01"]
			},
			{
				"property": "broadcastReply",
				"actionParams": ["true",
				"nx_more2"]
			},
			{
				"property": "backToMainMenu",
				"actionParams": []
			}]
		},
		{
			"condition": "isHaveBPInfo.是否存在记录 == true",
			"actions": [{
				"property": "executeStepAction",
				"actionParams": ["isStopService"]
			}]
		}],
		"helpReply": null,
		"reListenReply": null,
		"functionEntity": {
			"input": {
				"tel": "$tel$"
			},
			"functionName": "syllcx",
			"output": ["是否存在记录",
			"ttscontent",
			"smscontent"]
		}
	},
	{
		"property": "APIStep",
		"name": "isStopService",
		"transitions": [{
			"condition": "isStopService.是否停机==true",
			"actions": [{
				"property": "executeStepAction",
				"actionParams": ["上网流量查询"]
			}]
		},
		{
			"condition": "isStopService.是否停机==false",
			"actions": [{
				"property": "executeStepAction",
				"actionParams": ["sendMess"]
			},
			{
				"property": "executeStepAction",
				"actionParams": ["上网流量查询"]
			}]
		}],
		"helpReply": null,
		"reListenReply": null,
		"functionEntity": {
			"input": {
				"tel": "$tel$"
			},
			"functionName": "tjpd",
			"output": ["是否停机",
			"停机原因"]
		}
	},
	{
		"property": "APIStep",
		"name": "sendMess",
		"transitions": [],
		"helpReply": null,
		"reListenReply": null,
		"functionEntity": {
			"input": {
				"tel": "$tel$",
				"content": "$isHaveBPInfo.smscontent$"
			},
			"functionName": "dxxf",
			"output": []
		}
	},
	{
		"property": "commandASRStep",
		"name": "上网流量查询",
		"transitions": [{
			"condition": "command=='重听'",
			"actions": [{
				"property": "synthesisReply",
				"actionParams": [true,
				"$isHaveBPInfo.ttscontent$"]
			}]
		},
		{
			"condition": "command=='返回'",
			"actions": [{
				"property": "broadcastReply",
				"actionParams": ["true",
				"nx_more"]
			},
			{
				"property": "backToMainMenu",
				"actionParams": []
			}]
		}],
		"reListenReply": {
			"interrupt": true,
			"property": "synthesis",
			"content": "$isHaveBPInfo.ttscontent$"
		},
		"jumpOut": true,
		"noParamReply": {
			"interrupt": true,
			"property": "synthesis",
			"content": "$isHaveBPInfo.ttscontent$"
		},
		"checkParamJs": null,
		"commandList": ["重听"]
	},
	{
		"property": "jsStep",
		"name": "checkReply",
		"transitions": [{
			"condition": "isStopService.是否停机 == true && isStopService.停机原因 == '欠费停机'",
			"actions": [{
				"property": "broadcastReply",
				"actionParams": ["true",
				"nx_more1"]
			},
			{
				"property": "backToMainMenu",
				"actionParams": []
			}]
		},
		{
			"condition": "true",
			"actions": [{
				"property": "broadcastReply",
				"actionParams": ["true",
				"nx_more"]
			},
			{
				"property": "backToMainMenu",
				"actionParams": []
			}]
		}],
		"helpReply": null,
		"reListenReply": null,
		"script": "true"
	}]
}