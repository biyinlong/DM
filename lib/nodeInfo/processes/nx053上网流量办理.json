{
	"name": "nx053上网流量办理",
  "onEnterAction": [
    {
      "actionParams": [
        "false",
        "nx053_lb"
      ],
      "property": "broadcastReply"
    }
  ],
	"init_step": "isHavePackage",
	"steps": [{
		"property": "APIStep",
		"name": "isHavePackage",
		"transitions": [{
			"condition": "isHavePackage.是否有上网套餐==false",
			"actions": [{
				"property": "broadcastReply",
				"actionParams": ["false",
				"nx053_01"]
			},
			{
				"property": "broadcastReply",
				"actionParams": ["true",
				"nx0530101a"]
			}]
		},
		{
			"condition": "isHavePackage.是否有上网套餐==true",
			"actions": [{
				"property": "synthesisReply",
				"actionParams": ["false",
				"$isHavePackage.ttsContent$"]
			},
			{
				"property": "broadcastReply",
				"actionParams": ["true",
				"nx0530101b"]
			}]
		}],
		"helpReply": null,
		"reListenReply": null,
		"functionEntity": {
			"input": {
				"tel": "$tel$"
			},
			"functionName": "swtc",
			"output": ["是否有上网套餐",
			"ttsContent"]
		}
	}]
}